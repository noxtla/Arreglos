int j=9;
        for (int i=0; i<5; i++){
                System.out.print(numero[i] + " " + numero[j] + " ");
                j--;
        }
        
        System.out.println("\n");
                
        for (int i=0; i<5; i++){

                System.out.print(numero[i] +" "); 
                System.out.print(numero[9-i] +" ");
        }
                
        }
